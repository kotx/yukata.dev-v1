{
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run --unstable -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s",
    "dev": "deno task serve -d"
  },
  "imports": {
    "lume/": "https://deno.land/x/lume@v1.18.4/"
  },
  "fmt": {
    "useTabs": true,
    "semiColons": false
  }
}
